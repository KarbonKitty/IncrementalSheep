<p><strong>@Header:</strong></p>

<ul>
    @foreach (var resource in Resources.AllResources)
    {
        <li>@resource.Key: @resource.Value.FormatAsMoney()</li>
    }
</ul>

@code {
    [Parameter] public required SimplePrice Resources { get; set; }
    [Parameter] public required string Header { get; set; }
}
